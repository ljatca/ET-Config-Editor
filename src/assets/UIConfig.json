                    "6": {
                        "Condition": "#FlexPlan == false",
                        "Type": "MiddleSection",
                        "Description1": "$(ETCommon,CS_TRAD_CS_Credits_Group1)",
                        "Lines": {
                            "0": {
                                "Description1": "$(ETCommon,CS_TRAD_CS_Credits_Group1):",
                                "Value1": "@C2(#TotalMandatoryEmployeeCost+#TotalOptionalEmployeeCost) ##CostBasisSuffix"
                            },
                            "1": {
                                "Description1": "$(ETCommon,CS_Flex_CS_Costs_Group2):",
                                "Value1": "@C2(#TotalOptionalEmployeeCost) ##CostBasisSuffix"
                            }
                        }
                    },
                    "7": {
                        "Type": "LineBreak"
                    },
                    "8": {
                        "Type": "MiddleSection",
                        "Description1": "$(ETCommon,CS_TRAD_CS_TB_Title)",
                        "Lines": {
                            "0": {
                                "Description1": "$(ETCommon,CS_TRAD_CS_TB_FED):",
                                "Value1": "@C2(#TotalFederalTaxableBenfit) ##CostBasisSuffix"
                            },
                            "1": {
                                "Condition": "#IsQuebecEmployee",
                                "Description1": "$(ETCommon,CS_TRAD_CS_TB_PROV):",
                                "Value1": "@C2(#TotalProvincialTaxableBenefit) ##CostBasisSuffix"
                            }
                        }
                    },
                    "9": {
                        "Type": "Spacer1"
                    },
                    "10": {
                        "Type": "Footer",
                        "Description1": "$(ETCommon,CS_Flex_CS_Totals_Title)",
